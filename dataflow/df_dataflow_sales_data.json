					"dataset": {
						"referenceName": "ds_df_sales_drive_inputs",
						"type": "DatasetReference"
					},
					"name": "salesData"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
